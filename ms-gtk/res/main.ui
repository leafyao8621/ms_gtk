<interface>
    <object id="window" class="GtkWindow">
        <property name="visible">True</property>
        <property name="title">MS</property>
        <child>
            <object class="GtkGrid">
                <property name="visible">True</property>
                <child>
                    <object class="GtkGrid">
                        <property name="visible">True</property>
                        <property name="margin">5</property>
                        <child>
                            <object id="easy_start" class="GtkButton">
                                <property name="visible">True</property>
                                <property name="label">Easy</property>
                                <property name="width-request">200</property>
                            </object>
                            <packing>
                                <property name="left-attach">0</property>
                                <property name="top-attach">0</property>
                            </packing>
                        </child>
                        <child>
                            <object id="normal_start" class="GtkButton">
                                <property name="visible">True</property>
                                <property name="label">Normal</property>
                                <property name="width-request">200</property>
                            </object>
                            <packing>
                                <property name="left-attach">0</property>
                                <property name="top-attach">1</property>
                            </packing>
                        </child>
                        <child>
                            <object id="hard_start" class="GtkButton">
                                <property name="visible">True</property>
                                <property name="label">Hard</property>
                                <property name="width-request">200</property>
                            </object>
                            <packing>
                                <property name="left-attach">0</property>
                                <property name="top-attach">2</property>
                            </packing>
                        </child>
                    </object>
                    <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">0</property>
                    </packing>
                </child>
            </object>
        </child>
    </object>
</interface>